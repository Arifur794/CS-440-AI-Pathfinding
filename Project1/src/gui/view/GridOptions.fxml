<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<StackPane prefHeight="150.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.GridOptionsController">
                     <children>
                        <VBox id="button-menu" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
                           <children>
                              <Button fx:id="importMap" mnemonicParsing="false" text="Import Map">
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                                 <VBox.margin>
                                    <Insets top="10.0" />
                                 </VBox.margin>
                              </Button>
                              <Button fx:id="exportMap" alignment="CENTER" mnemonicParsing="false" text="Export Map">
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                                 <VBox.margin>
                                    <Insets bottom="10.0" top="10.0" />
                                 </VBox.margin>
                              </Button>
            <Button fx:id="findPath" alignment="CENTER" layoutX="69.0" layoutY="67.0" mnemonicParsing="false" text="Find Path">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
                              <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text-white" text="Map Number:" />
                              <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text-white" text="Start/Goal Number:" />
                           </children>
                        </VBox>
                     </children>
                  </StackPane>

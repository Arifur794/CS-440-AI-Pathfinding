<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane prefHeight="150.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.GridOptionsController">
                     <children>
                        <VBox id="button-menu" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
                           <children>
                              <Button fx:id="importMap" mnemonicParsing="false" text="Import Map">
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                                 <VBox.margin>
                                    <Insets bottom="10.0" top="10.0" />
                                 </VBox.margin>
                              </Button>
                              <Button fx:id="exportMap" alignment="CENTER" mnemonicParsing="false" text="Export Map">
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                                 <VBox.margin>
                                    <Insets bottom="10.0" top="10.0" />
                                 </VBox.margin>
                              </Button>
            <Button fx:id="findPath2" alignment="CENTER" layoutX="75.0" layoutY="151.0" mnemonicParsing="false" text="Find Uniform Cost Path">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
                                 <VBox.margin>
                                    <Insets bottom="10.0" top="10.0" />
                                 </VBox.margin>
            </Button>
            <Button fx:id="findPath" alignment="CENTER" layoutX="69.0" layoutY="67.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="147.0" text="Find A* path">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
                                 <VBox.margin>
                                    <Insets bottom="10.0" top="10.0" />
                                 </VBox.margin>
            </Button>
            <Button fx:id="findPath3" alignment="CENTER" layoutX="75.0" layoutY="114.0" mnemonicParsing="false" text="Find Weighted A* path">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
                                 <VBox.margin>
                                    <Insets bottom="10.0" top="10.0" />
                                 </VBox.margin>
            </Button>
            <TextField fx:id="weight" layoutX="10.0" layoutY="434.0" promptText="Enter weight W" text="2" />
            <Button fx:id="findPath4" alignment="CENTER" layoutX="40.0" layoutY="248.0" mnemonicParsing="false" text="Find all paths">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
            <Button fx:id="changeStart" alignment="CENTER" mnemonicParsing="false" prefHeight="37.0" prefWidth="202.0" text="Change Start Placement">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
                                 <VBox.margin>
                                    <Insets bottom="10.0" top="10.0" />
                                 </VBox.margin>
            </Button>
            <Button fx:id="changeEnd" alignment="CENTER" mnemonicParsing="false" prefHeight="37.0" prefWidth="210.0" text="Change End Placement">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
                                 <VBox.margin>
                                    <Insets bottom="10.0" top="10.0" />
                                 </VBox.margin>
            </Button>
            <TextField fx:id="row" promptText="Enter new start/end row" />
            <TextField fx:id="column" promptText="Enter new start/end column" />
                           </children>
                        </VBox>
                     </children>
                  </StackPane>

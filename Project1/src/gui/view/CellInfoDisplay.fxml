<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>

<StackPane prefHeight="150.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.CellInfoDisplayController">
                     <children>
                        <AnchorPane prefHeight="200.0" prefWidth="200.0" styleClass="anchor-pane">
                           <children>
                              <Label styleClass="label-bright" text="Cell Information:" AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="5.0" />
                              <GridPane AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="30.0">
                                <columnConstraints>
                                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                </columnConstraints>
                                <rowConstraints>
                                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                </rowConstraints>
                                 <children>
                                    <Label text="Coordinates" />
                                    <Label text="Cell Type" GridPane.rowIndex="1" />
                                    <Label fx:id="coordinates" text="" GridPane.columnIndex="1" />
                                    <Label fx:id="cellType" text="" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label fx:id="topLeft" GridPane.rowIndex="2" />
                  <Label fx:id="top" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label fx:id="topRight" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                  <Label fx:id="left" GridPane.rowIndex="3" />
                  <Label text="Cell" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <Label fx:id="right" GridPane.columnIndex="2" GridPane.rowIndex="3" />
                  <Label fx:id="botLeft" GridPane.rowIndex="4" />
                  <Label fx:id="bot" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <Label fx:id="botRight" GridPane.columnIndex="2" GridPane.rowIndex="4" />
                                 </children>
                              </GridPane>
                           </children>
                        </AnchorPane>
                     </children>
                  </StackPane>

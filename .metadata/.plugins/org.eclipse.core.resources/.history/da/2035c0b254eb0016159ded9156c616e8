package searches;

import gui.model.Cell;

public class Node extends Cell {
	
	private Node parent;
	public float gcost, hcost;
	public boolean isExplored;
	public Node[] neighborList;
	public boolean inFringe;
	
	public Node(Cell c) {
		super(c.x, c.y, c.celltype, c.dir);
		this.gcost = 0;
		this.hcost = 0;
		this.isExplored = false;	
		this.inFringe = false;
	}
	
	
}

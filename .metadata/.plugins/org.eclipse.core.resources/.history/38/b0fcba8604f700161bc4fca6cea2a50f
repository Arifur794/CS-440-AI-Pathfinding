package searches;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.PriorityQueue;

import gui.model.Grid;
import heuristics.Heuristic;

public class SAStar extends SearchAlgo {
	
	Node[] path; // path found by algorithm
	private ArrayList<PriorityQueue<Node>> allFringes; // fringe used by algorithm
	private Node[][] allNodes; // all explored nodes are stored here
	Node goal;
	Node start;
	Grid grid;
	private HashMap<Node, Node[]> mapOfNodes;
	
	
	public SAStar(Node start, Node goal, Grid grid, Heuristic[] hArray, float w1, float w2) {
		super(start, goal, grid);
	}
}
